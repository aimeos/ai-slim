{
	"name": "aimeos/ai-slim",
	"description": "Aimeos Slim PHP micro framework extension",
	"keywords": ["aimeos", "extension", "slim", "extension", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/SlimPHP",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-slim",
		"issues": "https://github.com/aimeos/ai-slim/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.5",
		"composer/installers": "^1.0",
		"aimeos/aimeos-core": "~2018.04"
	},
	"require-dev": {
		"slim/slim": "~3.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MW\\": "lib/mwlib/src/MW/"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
